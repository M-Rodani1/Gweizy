{
  "name": "crypto-gas-price-dashboard",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "sideEffects": [
    "**/*.css",
    "lucide-react/dist/esm/**/*.js",
    "lucide-react/**/*.js"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build && cp public/manifest.json dist/manifest.json 2>/dev/null || cp manifest.json dist/manifest.json 2>/dev/null || true && cp public/_redirects dist/_redirects 2>/dev/null || true && cp public/_headers dist/_headers 2>/dev/null || true && cp public/.cloudflareignore dist/.cloudflareignore 2>/dev/null || true",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test-storybook": "test-storybook",
    "test-storybook:ci": "test-storybook --ci",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "prepare": "husky",
    "analyze": "ANALYZE=true vite build",
    "generate:component": "node scripts/generate-component.js",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report"
  },
  "dependencies": {
    "@farcaster/miniapp-sdk": "^0.2.1",
    "@sentry/react": "^10.32.1",
    "@tanstack/react-query": "^5.62.0",
    "@tanstack/react-virtual": "^3.13.18",
    "framer-motion": "^12.23.26",
    "lucide-react": "^0.561.0",
    "react": "^18.3.1",
    "react-countup": "^6.5.3",
    "react-dom": "^18.3.1",
    "react-hot-toast": "^2.4.1",
    "react-is": "^18.3.1",
    "react-router-dom": "^7.10.1",
    "recharts": "^3.5.1",
    "socket.io-client": "^4.8.3",
    "zod": "^4.3.6",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@axe-core/react": "^4.11.1",
    "@playwright/test": "^1.58.2",
    "@storybook/addon-essentials": "^8.6.15",
    "@storybook/addon-interactions": "^8.6.15",
    "@storybook/addon-links": "^8.6.15",
    "@storybook/react-vite": "^8.6.15",
    "@storybook/test": "^8.6.15",
    "@storybook/test-runner": "^0.24.2",
    "@tailwindcss/postcss": "^4.1.18",
    "@tanstack/react-query-devtools": "^5.91.1",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@types/jest-axe": "^3.5.9",
    "@types/jest-image-snapshot": "^6.4.1",
    "@types/node": "^22.14.0",
    "@types/react": "^19.2.11",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.23",
    "eslint": "^10.0.0",
    "eslint-plugin-react-hooks": "^7.0.1",
    "husky": "^9.1.7",
    "jest-axe": "^10.0.0",
    "jest-image-snapshot": "^6.5.1",
    "jsdom": "^23.0.1",
    "lint-staged": "^16.2.7",
    "msw": "^2.12.9",
    "postcss": "^8.5.6",
    "rollup-plugin-visualizer": "^6.0.5",
    "storybook": "^8.6.15",
    "tailwindcss": "^4.1.18",
    "terser": "^5.44.1",
    "typescript": "~5.8.2",
    "vite": "^6.2.0",
    "vite-plugin-sri": "^0.0.2",
    "vitest": "^4.0.16",
    "vitest-axe": "^0.1.0",
    "zustand": "^5.0.2"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix"
    ]
  }
}
